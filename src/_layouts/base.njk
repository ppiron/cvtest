<html lang="en">
  <head>
    <meta charset="UTF-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <title>Document</title>
    <link rel="stylesheet" href="/css/style.css" />
    <script src="https://cdnjs.cloudflare.com/ajax/libs/gsap/3.5.1/gsap.min.js"></script>
    <script src="https://cdnjs.cloudflare.com/ajax/libs/gsap/3.5.1/ScrollToPlugin.min.js"></script>
    <script src="https://identity.netlify.com/v1/netlify-identity-widget.js"></script>
    <script
      src="https://cdn.jsdelivr.net/gh/alpinejs/alpine@v2.x.x/dist/alpine.min.js"
      defer
    ></script>
  </head>
  <body x-data="{}">
    <header>
      <div class="img_container">
        <img src="/img/photo for website.png" alt="myself" />
      </div>
      <nav>
        <ul>
          <li>
            <a
              href="#about"
              @click.prevent="gsap.to(window, {duration: 1, scrollTo:'#about', ease: 'expo.in'});"
              >About</a
            >
          </li>
          <li>
            <a
              href="#experience"
              @click.prevent="gsap.to(window, {duration: 1, scrollTo:'#experience', ease: 'expo.in'});"
              >Experience</a
            >
          </li>
          <li>
            <a
              href="#education"
              @click.prevent="gsap.to(window, {duration: 1, scrollTo:'#education', ease: 'expo.in'});"
              >Education</a
            >
          </li>
          <li>
            <a
              href="#skills"
              @click.prevent="gsap.to(window, {duration: 1, scrollTo:'#skills', ease: 'expo.in'});"
              >Skills</a
            >
          </li>
          <li>
            <a
              href="#interests"
              @click.prevent="gsap.to(window, {duration: 1, scrollTo:'#interests', ease: 'expo.in'});"
              >Interests</a
            >
          </li>
          <li>
            <a
              href="#awards"
              @click.prevent="gsap.to(window, {duration: 1, scrollTo:'#awards', ease: 'expo.in'});"
              >Awards</a
            >
          </li>
        </ul>
      </nav>
    </header>
    {{ content | safe }}

    <!-- <script>
      if (window.netlifyIdentity) {
        window.netlifyIdentity.on("init", (user) => {
          if (!user) {
            window.netlifyIdentity.on("login", () => {
              document.location.href = "/admin/";
            });
          }
        });
      }
    </script> -->
  </body>
</html>
